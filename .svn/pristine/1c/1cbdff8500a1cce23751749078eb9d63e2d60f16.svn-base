<!DOCTYPE html>
<html lang="en">
<body>
<wicket:extend>
	<!-- MENU -->
    <div class="navbar-wrapper">
      <div class="container">		  
		  <div wicket:id="facturarOnLineMenu"></div>
      </div>
    </div>

    <!-- Marketing messaging and featurettes
    ================================================== -->
    <!-- Wrap the rest of the page in another container to center all the content. -->
    <div class="container marketing">
		
		<div class="panel panel-default">
			<div class="panel-heading">
				<h2 class="panel-title">
					<img src="images/cobranzas-64.png" alt="Cobranzas"/>
					Cobranzas
					<small><span>Editar Fechas Cheques</span></small>
				</h2>
			</div>
			<div class="panel-body" style="width: 70%;">
			
				<form class="form-horizontal" role="form" wicket:id="form">		

				  <div class="errorLogin" wicket:id="feedback"></div>
				  				  
				  <div>
					  <div class="page-header">
					  	<h3>Cheques</h3>
					  </div>
					  <div>
					  	  <small><em><u>Nota</u>: En importes indicar decimales con coma.</em></small>
						  <table class="table">
							<thead>
								<tr>
									<td class="col-xs-3"><b>Nro Cheque</b></td>
									<td class="col-xs-3"><b>Banco</b></td>
									<td class="col-xs-3"><b>Importe</b></td>
									<td class="col-xs-3"><b>Fecha</b></td>
								</tr>
							</thead>
							<tbody>
								<tr wicket:id="listaCheques">
									<td>
										<span wicket:id="nroCheque"></span>
									</td>
									<td>
										<span wicket:id="bancoCheque"></span>
									</td>
									<td>
										<span wicket:id="importeCheque"></span>
									</td>
									<td>
										<input type="text" class="form-control" placeholder="Haga click aqu&iacute;" wicket:id="datepickerCheque" autocomplete="off"></input>
									</td>
								</tr>
							</tbody>
						</table>
					  </div>
				  </div>
				  				  
				  <div class="col-md-5 col-md-offset-9">
				  	<button type="submit" class="btn btn-info">Guardar Cambios</button>
					<a href="cobranzas" class="btn btn-default">Volver</a>
				  </div>
				</form>				
					
			</div>
		</div>

    </div><!-- /.container -->
	
</wicket:extend>
</body>
</html>