<!DOCTYPE html>
<html lang="en">
<body>
<wicket:extend>
	<!-- MENU -->
    <div class="navbar-wrapper">
      <div class="container">		  
		  <div wicket:id="facturarOnLineMenu"></div>
      </div>
    </div>

    <!-- Marketing messaging and featurettes
    ================================================== -->
    <!-- Wrap the rest of the page in another container to center all the content. -->
    <div class="container marketing">

		<div wicket:id="alerta" class="alert alert-success alert-dismissable">
		  <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
		  <span wicket:id="textoAlerta">Texto Alerta</span>
		</div>

		<div class="panel panel-default">
			<div class="panel-heading">
				<h2 class="panel-title">
					<img src="images/clientes-64.png" alt="Clientes"/>
					Clientes
					<small><span wicket:id="secondaryTitle">Crear</span></small>
				</h2>
			</div>
			<div class="panel-body" style="width: 70%;">
			
				<div class="errorLogin" wicket:id="feedback"></div>
			
				<form class="form-horizontal" role="form" wicket:id="form">
				  <div class="form-group">
				    <label class="col-sm-2 control-label">Raz&oacute;n social<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control" placeholder="Raz&oacute;n social" wicket:id="razonSocial" autocomplete="off">
				    </div>
				  </div>
				  <div class="form-group">
				    <label class="col-sm-2 control-label">CUIT / CUIL<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control" placeholder="CUIT / CUIL" wicket:id="cuitCuil" autocomplete="off">
					  <span id="helpBlock" class="help-block">Ejemplo Formato: 23-12345678-1</span>
				    </div>
				  </div>
				  
				  
				  <div class="page-header">
				  	<h1><small>Modos de cobro</small></h1>
				  </div>
				  
				  <div class="form-group">
				    <label class="col-sm-2 control-label">Seleccione<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    <div class="col-sm-10">
						<div class="checkbox">
							<label>
								<input type="checkbox" wicket:id="modoSinFactura">
							</label>
						</div>
						<div class="checkbox">
							<label>
								<input type="checkbox" wicket:id="modoEfectivo">Efectivo
							</label>
						</div>
						<div class="checkbox">
							<label>
								<input type="checkbox" wicket:id="modoCheque">Cheque
							</label>
						</div>
						<div class="checkbox">
							<label>
								<input type="checkbox" wicket:id="modoTransferencia">Transferencia
							</label>
						</div>
					</div>
				  </div>
				  
				  <div class="page-header">
				  	<h1><small>Datos de cobro</small></h1>
				  </div>
				  
				  <div class="form-group">
				    <label class="col-sm-2 control-label">Domicilio</label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control" placeholder="Domicilio" wicket:id="domicilio" autocomplete="off">
				    </div>
				  </div>
				  
				  <div class="form-group">
				    <label class="col-sm-2 control-label">Telefono 1</label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control" placeholder="Telefono 1" wicket:id="telefono1" autocomplete="off">
				    </div>
				  </div>
				  
				  <div class="form-group">
				    <label class="col-sm-2 control-label">Telefono 2</label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control" placeholder="Telefono 2" wicket:id="telefono2" autocomplete="off">
				    </div>
				  </div>
				  
				  <div class="form-group">
				    <label class="col-sm-2 control-label">Contacto</label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control" placeholder="Contacto" wicket:id="contacto" autocomplete="off">
				    </div>
				  </div>
				  
				  <div class="form-group">
				    <label class="col-sm-2 control-label">Mails</label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control" placeholder="Mails" wicket:id="mails" autocomplete="off">
				    </div>
				  </div>
				  
				  <div class="form-group">
				    <label class="col-sm-2 control-label">Referencias</label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control" placeholder="Referencias" wicket:id="referencias" autocomplete="off">
				    </div>
				  </div>
				  
				  <div class="form-group">
				  	<label class="col-sm-2 control-label">Observaciones</label>
				  	<div class="col-sm-10" style="width: 70%;">
						<textarea class="form-control" rows="3" placeholder="Observaciones" wicket:id="observaciones" autocomplete="off"></textarea>
					</div>
				  </div>
				  
				  
				  <div class="row">
					  <div class="form-group pull-right">					  	
					  	<input type="submit" wicket:id="guardar" value="Guardar" class="btn btn-success"/>
					  	<button class="btn btn-default" wicket:id="cancelarLink">Cancelar</button>
					  </div>
				  </div>
				  			  
				</form>
					
			</div>
		</div>

    </div><!-- /.container -->
	
</wicket:extend>
</body>
</html>