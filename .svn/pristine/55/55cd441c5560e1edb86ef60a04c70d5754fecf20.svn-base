<!DOCTYPE html>
<html lang="en">
<body>
<wicket:extend>
	<!-- MENU -->
    <div class="navbar-wrapper">
      <div class="container">		  
		  <div wicket:id="facturarOnLineMenu"></div>
      </div>
    </div>

    <!-- Marketing messaging and featurettes
    ================================================== -->
    <!-- Wrap the rest of the page in another container to center all the content. -->
    <div class="container marketing">

		<div class="panel panel-success">
			<div class="panel-heading">
				<h2 class="panel-title">
					<img src="images/contacto.png" alt="Cont&aacute;ctenos"/>
					Trebol4 SRL
					<small>Solicite su plan / Cont&aacute;ctenos</small>
				</h2>
			</div>
			<div class="panel-body" style="width: 70%;">
			
				<div class="errorLogin" wicket:id="feedback"></div>
			
				<form class="form-horizontal" role="form" wicket:id="form">
				  <div class="form-group">
				    <label class="col-sm-2 control-label">Mail<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    <div class="col-sm-10" style="width: 40%;">
				      <input type="text" class="form-control" placeholder="Escriba aqu&iacute; su email" wicket:id="para">
				    </div>
				  </div>
				  <div class="form-group">
				  	<label class="col-sm-2 control-label">Asunto<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    <div class="col-sm-10" style="width: 40%;">
					   <select class="form-control" wicket:id="asunto">
                           <option>Consulta planes</option>
                           <option>Contratar plan</option>
                           <option>Otros</option>
						</select>
					</div>
				  </div>
				  <div class="form-group">
				    <label class="col-sm-2 control-label">Mensaje<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    <div class="col-sm-10">
				      <textarea rows="10" class="form-control" placeholder="Indique aqu&iacute; el plan a solicitar, o las consultas que usted desee" wicket:id="cuerpo"></textarea>
				    </div>
				  </div>
				  <div class="form-group" wicket:id="captchaContainer">
				  	<label class="col-sm-2 control-label">&nbsp;</label>
				    <div class="col-sm-10">
				    	<img wicket:id="image" />
		                <a wicket:id="changeLink">Cambiar</a>
		                <br />
		                Replique aqu&iacute; abajo el texto de la im&aacute;gen.
		                <br />
		                <input type="text" class="form-control" placeholder="Escriba aqu&iacute; el texto" wicket:id="captchaText" style="width: 30%;"/>
				    </div>
				  </div>
				  <div class="form-group pull-right">
					<a href="login" class="btn btn-default">Cancelar</a>
				  	<button type="submit" class="btn btn-success">Enviar</button>
				  </div>
				</form>
					
			</div>
		</div>

    </div><!-- /.container -->
	
</wicket:extend>
</body>
</html>