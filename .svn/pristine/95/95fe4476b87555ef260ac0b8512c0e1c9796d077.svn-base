<!DOCTYPE html>
<html lang="en">
<body>
<wicket:extend>
	<!-- MENU -->
    <div class="navbar-wrapper">
      <div class="container">		  
		  <div wicket:id="facturarOnLineMenu"></div>
      </div>
    </div>

    <!-- Marketing messaging and featurettes
    ================================================== -->
    <!-- Wrap the rest of the page in another container to center all the content. -->
    <div class="container marketing">

		<div wicket:id="alerta" class="alert alert-success alert-dismissable">
		  <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
		  <span wicket:id="textoAlerta">Texto Alerta</span>
		</div>

		<div class="panel panel-default">
			<div class="panel-heading">
				<h2 class="panel-title">
					<img src="images/usuario.png" alt="Usuarios"/>
					Usuarios
					<small><span wicket:id="secondaryTitle">Crear</span></small>
				</h2>
			</div>
			<div class="panel-body" style="width: 70%;">
			
				<div class="errorLogin" wicket:id="feedback"></div>
			
				<form class="form-horizontal" role="form" wicket:id="form">
				  <div class="form-group">
				    <label class="col-sm-2 control-label">Nombre<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control" placeholder="Nombre / Raz&oacute;n social" wicket:id="nombre" autocomplete="off">
				    </div>
				  </div>
				  <div class="form-group">
				    <label class="col-sm-2 control-label">Apellido<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control" placeholder="Apellido" wicket:id="apellido" autocomplete="off">
				    </div>
				  </div>
				  <div class="form-group">
				    <label class="col-sm-2 control-label">Email<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control" placeholder="Email" wicket:id="email" autocomplete="off">
				    </div>
				  </div>
				  
			 	  <wicket:enclosure child="clave">
                      <div class="form-group">
			 	          <label class="col-sm-2 control-label">Clave<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
		  		          <div class="col-sm-10">
	   			          <input type="password" class="form-control" placeholder="Clave" wicket:id="clave" autocomplete="off">
    				      </div>
                      </div>
				  </wicket:enclosure>
				  
				  <div class="form-group">
				    <label class="col-sm-2 control-label">Tel&eacute;fono</label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control" placeholder="Tel&eacute;fono" wicket:id="telefono" autocomplete="off">
				    </div>
				  </div>
				  <!-- <div class="form-group" wicket:id="rolContainer">
				  	<label class="col-sm-2 control-label">Rol<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    <div class="col-sm-10">
					    <select class="form-control" wicket:id="rol">
						  <option>Administrador</option>
						  <option>UsuarioWeb</option>
						</select>
						</div>
				  </div>-->
				  <div class="form-group" wicket:id="rolContainer">
				  	<label class="col-sm-2 control-label">Roles<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    <div class="col-sm-10">
					    <span wicket:id="listaRolesMultipleChoice">ListMultipleChoiceTransfer will be placed here</span>
					</div>
				  </div>
				  <div class="form-group pull-right">
				  	<button type="submit" class="btn btn-success">Guardar</button>
				  	<button class="btn btn-default" wicket:id="cancelarLink">Cancelar</button>
				  </div>
				</form>
					
			</div>
		</div>

    </div><!-- /.container -->
	
</wicket:extend>
</body>
</html>