<!DOCTYPE html>
<html lang="en">
<body>
<wicket:extend>
	<!-- MENU -->
    <div class="navbar-wrapper">
      <div class="container">		  
		  <div wicket:id="facturarOnLineMenu"></div>
      </div>
    </div>

    <!-- Marketing messaging and featurettes
    ================================================== -->
    <!-- Wrap the rest of the page in another container to center all the content. -->
    <div class="container marketing">
    
    	<div wicket:id="alerta" class="alert alert-success alert-dismissable">
		  <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
		  <span wicket:id="textoAlerta">La factura ha sido eliminada.</span>
		</div>

		<div class="panel panel-default">
			<div class="panel-heading">
				<h2 class="panel-title">
					<img src="images/facturas-64.png" alt="Facturas"/>
					Facturas Compra
					<small><span wicket:id="secondaryTitle">Crear</span></small>
				</h2>
			</div>
			<div class="panel-body" style="width: 70%;">
			
				<div class="errorLogin" wicket:id="feedback"></div>
			
				<form class="form-horizontal" role="form" wicket:id="form">
				
				  <div class="form-group">
				    	<label class="col-sm-3 control-label">Fecha<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    	<div class="col-sm-10" style="width: 40%;">
							<input type="text" class="form-control" placeholder="Haga click aqu&iacute;" wicket:id="datepicker" autocomplete="off"></input>
						</div>
						<div class="row">
							<h6>
								<a wicket:id="hoy" href="#"><small>Hoy</small></a>
							</h6>
						</div>
				  </div>
				  <div class="form-group">
				  	<label class="col-sm-3 control-label">Mes Impositivo<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    <div class="col-sm-10" style="width: 40%;">
					   <select class="form-control" wicket:id="mesImpositivo">
                           <option>Nov / 2015</option>
                           <option>Dic / 2015</option>
						</select>
					</div>
				  </div>
				  <div class="form-group">
				    <label class="col-sm-3 control-label">Proveedor<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    	<div class="col-sm-10" style="width: 40%;">
				    		<input type="text" class="form-control" placeholder="Ingrese al menos 3 d&iacute;gitos" size="30" wicket:id="proveedorSearchAutocomplete"  data-toggle="tooltip" title="Utilice la raz&oacute;n social" autocomplete="off"></input>
						</div>
				  </div>
				  <div class="form-group">
				  	<label class="col-sm-3 control-label">Tipo Factura<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    <div class="col-sm-10" style="width: 40%;">
					   <select class="form-control" wicket:id="tipo">
                           <option>A</option>
                           <option>B</option>
						</select>
					</div>
				  </div>
				  <div class="form-group">
				    <label class="col-sm-3 control-label">Nro<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
				    <div class="col-sm-10" style="width: 40%;">
				      <input type="text" class="form-control" placeholder="Ejemplo: 00002-00000135" wicket:id="nro" autocomplete="off">
				    </div>
				  </div>
				  <small class="pull-right"><em><u>Nota</u>: En importes indicar decimales con coma.</em></small>
				  <div class="form-group">
				    <label class="col-sm-3 control-label">SubTotal<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
					    <div class="col-sm-10" style="width: 30%;">
				    		<div class="input-group"><span class="input-group-addon">$</span><input type="text" class="form-control text-right" placeholder="SubTotal" wicket:id="subtotal" autocomplete="off" onClick="this.select()"/></div>
				    	</div>
				  </div>
				  <div class="form-group">
				    <label class="col-sm-3 control-label">IVA<small data-toggle="tooltip" title="Campo obligatorio">&nbsp;*</small></label>
					    <div class="col-sm-10" style="width: 30%;">
				    		<div class="input-group"><span class="input-group-addon">$</span><input type="text" class="form-control text-right" placeholder="IVA" wicket:id="iva" autocomplete="off" onClick="this.select()"></div>
						</div>
				  </div>
				  
				    <table class="table">
						<thead>
							<tr>
								<td><b>PERC. IVA</b></td>
								<td><b>PERC. IIBB</b></td>
								<td><b>PERC. GCIAS</b></td>
								<td><b>PERC. SUSS</b></td>
								<td><b>Otras PERC.</b></td>
							</tr>
						</thead>
						<tbody>
						  <tr>
							<td>
								<div class="input-group"><span class="input-group-addon">$</span><input type="text" class="form-control text-right" placeholder="PERC. IVA" wicket:id="percIva" autocomplete="off" onClick="this.select()"></div>
							</td>
							<td>
								<div class="input-group"><span class="input-group-addon">$</span><input type="text" class="form-control text-right" placeholder="PERC. IIBB" wicket:id="percIibb" autocomplete="off" onClick="this.select()"></div>
							</td>
							<td>
								<div class="input-group"><span class="input-group-addon">$</span><input type="text" class="form-control text-right" placeholder="PERC. GCIAS" wicket:id="percGcias" autocomplete="off" onClick="this.select()"></div>
							</td>
							<td>
								<div class="input-group"><span class="input-group-addon">$</span><input type="text" class="form-control text-right" placeholder="PERC. SUSS" wicket:id="percSUSS" autocomplete="off" onClick="this.select()"></div>
							</td>
							<td>
								<div class="input-group"><span class="input-group-addon">$</span><input type="text" class="form-control text-right" placeholder="OTRAS PERC." wicket:id="otrasPerc" autocomplete="off" onClick="this.select()"></div>
							</td>
						  </tr>
						</tbody>
					</table>

				  <div class="form-group has-error">
				    <label class="col-sm-3 control-label">Total</label>
					    <div class="col-sm-10" style="width: 30%;">
				    		<div class="input-group"><span class="input-group-addon">$</span><p class="form-control text-right" wicket:id="total"></p></div>
						</div>
				  </div>
				  <div class="form-group" wicket:id="estadoContainer">
				    <label class="col-sm-3 control-label">Estado</label>
					    <div class="col-sm-10" style="width: 30%;">
				    		<div class="input-group"><span class="input-group-addon" wicket:id="estado">Estado</span></div>
						</div>
				  </div>
				  <div class="form-group pull-right">
				  	<button type="submit" class="btn btn-success" wicket:id="guardar">Guardar</button>
					<a href="facturasCompras" class="btn btn-default">Volver</a>
				  </div>
				</form>
					
			</div>
		</div>

    </div><!-- /.container -->
	
</wicket:extend>
</body>
</html>