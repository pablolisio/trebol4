<wicket:panel>

   


     <div class="container">
     <div class="col-md-4 col-md-offset-4">
		
		
			<div class="panel panel-primary">
				<div class="panel-heading text-center">
					<img src="images/LogoTrebolTransparente.png" alt="Trebol4 SRL">
				</div>
			</div>
			
 	<form  wicket:id="signInForm">

			<h2 class="form-signin-heading">Por favor ingrese</h2>
			
	    	<div class="form-group">
	            <input wicket:id="username" type="text" placeholder="Usuario" class="form-control" autofocus>
	        </div>
	        <div class="form-group">
	            <input wicket:id="password" type="password" placeholder="Clave" class="form-control">
	        </div>
	        
	        <div wicket:id="rememberMeRow" class="checkbox form-group">
	            <input wicket:id="rememberMe" type="checkbox">
	            Acordarse de mi
	        </div>
	        
	        <button type="submit" class="btn btn-lg btn-primary btn-block" data-loading-text="Espere..." >Ingresar</button>
	        
	      	<!-- El siguiente modal referencia a un <div> que se encuentra en LoginPage.html -->
	        <a href="#" data-toggle="modal" data-target="#olvidoClaveModal"><small><em>Olvidaste tu clave?</em></small></a>
	        
    </form>
    <div class="errorLogin" wicket:id="feedback"></div>
    </div>
    </div> <!-- /container -->
    
    <br/><br/>
    
</wicket:panel>